var paymentConcludedModal = $('<%= j render "concluded_payment", item: @context.item, total_change: @context.total_change %>')

$("#itemPayment").modal('hide');

$('#itemListing').modalmanager('loading');

$('#itemListing').modal().find('.modal-body').html('<%= j render "items/index", items: @vending_machine.items.order(id: :asc) %>');

$("#concludedPayment").remove();
$("body").append(paymentConcludedModal);
$("#concludedPayment").modal();